{
  "name": "medpriest-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:init": "wrangler d1 execute medpriest-db --local --file=schema.sql",
    "db:seed": "wrangler d1 execute medpriest-db --local --file=seed.sql",
    "db:init:remote": "wrangler d1 execute medpriest-db --remote --file=schema.sql",
    "db:seed:remote": "wrangler d1 execute medpriest-db --remote --file=seed.sql"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "wrangler": "^3.99.0"
  }
}
